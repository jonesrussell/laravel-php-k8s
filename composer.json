{
    "name": "jonesrussell/laravel-php-k8s",
    "description": "Just a simple port of renoki-co/php-k8s for easier access in Laravel.",
    "keywords": [
        "laravel",
        "php",
        "laravel",
        "kubernetes",
        "k8s",
        "k3s",
        "kube"
    ],
    "license": "Apache-2.0",
    "homepage": "https://github.com/jonesrussell/laravel-php-k8s",
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        },
        {
            "name": "Russell Jones",
            "homepage": "https://github.com/jonesrussell",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jonesrussell/php-k8s.git"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-yaml": "*",
        "ext-curl": "*",
        "illuminate/support": "^11.0",
        "jonesrussell/php-k8s": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "RenokiCo\\LaravelK8s\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenokiCo\\LaravelK8s\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "mockery/mockery": "^1.6.12",
        "orchestra/testbench": "^9.0",
        "orchestra/testbench-core": "^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "RenokiCo\\LaravelK8s\\LaravelK8sServiceProvider"
            ]
        },
        "laravel-zero": {
            "providers": [
                "RenokiCo\\LaravelK8s\\LaravelK8sServiceProvider"
            ]
        }
    }
}
